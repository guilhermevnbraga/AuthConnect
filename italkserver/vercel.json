{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./index.js",
            "builds": [{ "src": "index.js", "use": "@vercel/node" }],
            "scripts": {
                "build": "npm run build"
            },
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
